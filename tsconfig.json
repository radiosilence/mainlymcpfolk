{
	"compilerOptions": {
		"target": "ESNext",
		"module": "ESNext",
		"moduleResolution": "bundler",
		"strict": true,
		"noEmit": true,
		"isolatedModules": true,
		"verbatimModuleSyntax": true,
		"skipLibCheck": true,
		"noUncheckedIndexedAccess": true,
		"noUncheckedSideEffectImports": true,
		"erasableSyntaxOnly": true
	},
	"include": ["src/**/*"]
}
